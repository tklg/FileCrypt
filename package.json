{
  "name": "filecrypt",
  "version": "1.2.0",
  "description": "a webcrypto wrapper for files",
  "main": "lib/index.js",
  "scripts": {
    "test": "set NODE_ENV=development && gulp test",
    "test-compile": "set NODE_ENV=development && gulp compile:test",
    "build": "set NODE_ENV=production && gulp --env=production --single",
    "start": "./node_modules/.bin/gulp",
    "sel": "selenium-standalone",
    "sel-install": "selenium-standalone install --config=./selenium.conf.js",
    "selenium": "selenium-standalone start",
    "prepublish2": "set NODE_ENV=production && gulp prepublish --env=production",
    "prepublish": "set NODE_ENV=production && webpack"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "file-saver": "^1.3.3",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "mocha": "^3.4.2",
    "prop-types": "^15.5.10",
    "selenium-standalone": "^6.4.1",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.8.0",
    "webpack": "^2.6.1",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es2015-destructuring",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/villa7/filecrypt"
  },
  "author": "villa7",
  "keywords": [
    "file",
    "encrypt",
    "decrypt",
    "webcrypto",
    "crypto"
  ]
}
